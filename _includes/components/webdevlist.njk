<section>
	{% for webdev in webdevlist | reverse %}
		<article{% if webdev.url == url %} data-current="current item"{% endif %}>
			<h3>
				<a href="{{ webdev.url | url }}">
					{% if webdev.data.title %}
						{{ webdev.data.title }}
					{% else %}
						Untitled
					{% endif %}
				</a>
			</h3>
			<img src="{{ webdev.data.image }}" alt="{{ webdev.data.title }}: web development by David Rhoden" />
			{{ webdev.data.body }}
			{% if webdev.data.tags %}
				<p>
					{% for tag in webdev.data.tags %}
		  			{%- if tag != "webdev" -%}
		  				{% set tagUrl %}/tags/{{ tag }}/{% endset %}
		  			<a href="{{ tagUrl | url }}" rel="tag">{{ tag }}</a>
		  			{%- endif -%}
					{% endfor %}
				</p>
			{% endif %}
		</article>
	{% endfor %}
</section>
